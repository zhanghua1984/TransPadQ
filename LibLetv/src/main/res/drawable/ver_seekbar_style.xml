<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="10dip" />

            <gradient
                android:angle="270"
                android:centerColor="@color/letv_color_00000000"
                android:centerY="0.75"
                android:endColor="@color/letv_color_00000000"
                android:startColor="@color/letv_color_00000000" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip
            android:clipOrientation="vertical"
            android:gravity="bottom" >
            <shape>
                <corners android:radius="10dip" />

                <gradient
                    android:angle="270"
                    android:centerColor="@color/letv_color_00000000"
                    android:centerY="0.75"
                    android:endColor="@color/letv_color_00000000"
                    android:startColor="@color/letv_color_00000000" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip
            android:clipOrientation="vertical"
            android:gravity="bottom" >
            <shape>
                <corners android:radius="10dip" />

                <gradient
                    android:angle="270"
                    android:centerColor="@color/letv_color_ff00a0e9"
                    android:centerY="0.75"
                    android:endColor="@color/letv_color_ff00a0e9"
                    android:startColor="@color/letv_color_ff00a0e9" />
            </shape>
        </clip>
    </item>

</layer-list>