<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="257dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:layout_centerHorizontal="true"
        android:background="@drawable/dialog_background"
        android:layout_centerVertical="true"
        android:orientation="vertical">
    <LinearLayout
        android:layout_marginLeft="20dp"
        android:id="@+id/rl_menu"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:gravity="center"
        >
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            >
            <View
                android:layout_width="66dp"
                android:layout_marginLeft="3dp"
                android:layout_height="38dp"
                android:layout_centerVertical="true"
                android:background="@drawable/fm_background"/>
            <cn.transpad.transpadui.view.FmPickerView
                android:id="@+id/picker_view1"
                android:layout_width="70dp"
                android:layout_height="140dp" />
        </RelativeLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="."
            android:layout_marginLeft="5dp"
            android:layout_marginRight="2dp"
            android:textColor="@color/white"
            android:textSize="20sp"
            android:textStyle="bold" />
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            >
            <View
                android:layout_width="66dp"
                android:layout_height="38dp"
                android:layout_marginLeft="3dp"
                android:layout_centerVertical="true"
                android:background="@drawable/fm_background"/>
            <cn.transpad.transpadui.view.FmPickerView
                android:id="@+id/picker_view2"
                android:layout_width="70dp"
                android:layout_height="140dp"
                />
        </RelativeLayout>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="MHz"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="2dp"
            android:textColor="@color/white"
            android:textSize="20sp"
            android:textStyle="bold" />
    </LinearLayout>
    <Button
    android:layout_width="207dp"
    android:layout_height="27dp"
    android:text="@string/finish"
    android:id="@+id/bt_ok"
    android:layout_centerHorizontal="true"
    android:textSize="13sp"
    android:layout_marginTop="8dp"
    android:textColor="@color/white"
    android:background="@drawable/dialog_button_ok"
    android:layout_below="@+id/ll_four_np"
    android:layout_marginBottom="8dp"/>
    </LinearLayout>
